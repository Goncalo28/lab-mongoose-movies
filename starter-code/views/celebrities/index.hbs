<h2>Celebrities</h2>
{{#each celebrities}}
    <p>{{name}}</p>
    <a href="/celebrities/{{_id}}">Details</a>
    <a href="/celebrities/new">Create a celebrity</a>
    <a href="/celebrities/{{_id}}/edit">Edit</a>
    <form action="celebrities/{{_id}}/delete" method="POST">
        <button type="submit">Delete</button>
    </form>
{{/each}}
